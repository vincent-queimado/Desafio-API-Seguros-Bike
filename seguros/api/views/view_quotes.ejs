  	<script>
	
	//*** Check JS Plugins
	if (!$().steps) {
			console.warn('Warning - steps.min.js is not loaded.');
	}

	if (!$().validate) {
			console.warn('Warning - validate.min.js is not loaded.');
	}
	
	//*** Wizard Form
	var form_addclient = $("#addForm").show();
 
	form_addclient.steps({
		headerTag: "h3",
		bodyTag: "fieldset",
		transitionEffect: "slideLeft",
		titleTemplate: '<span class="number">#index#</span> #title#',
		labels: {
					previous: '<i class="icon-arrow-left13 mr-2" /> Anterior',
					next: 'Próximo <i class="icon-arrow-right14 ml-2" />',
					finish: 'Realizar Cotação<i class="icon-plus2 ml-2" />'
			},
			transitionEffect: 'fade',
			autoFocus: true,
			onStepChanging: function (event, currentIndex, newIndex) {

					// Allways allow previous action even if the current form is not valid!
					if (currentIndex > newIndex) {
							return true;
					}

					// Needed in some cases if the user went back (clean up)
					if (currentIndex < newIndex) {

							// To remove error styles
							form_addclient.find('.body:eq(' + newIndex + ') label.error').remove();
							form_addclient.find('.body:eq(' + newIndex + ') .error').removeClass('error');
					}

					form_addclient.validate().settings.ignore = ':disabled,:hidden';
					return form_addclient.valid();
			},
			onFinishing: function (event, currentIndex) {
					form_addclient.validate().settings.ignore = ':disabled';
					return form_addclient.valid();
			},
			onFinished: function (event, currentIndex) {
					//alert('Cadastrado!');
					form_addclient.submit();
			}
	});
	
	
	
	//*** Initialize validation
	form_addclient.validate({
			ignore: 'input[type=hidden], .select2-search__field', // ignore hidden fields
			errorClass: 'validation-invalid-label',
			highlight: function(element, errorClass) {
					$(element).removeClass(errorClass);
			},
			unhighlight: function(element, errorClass) {
					$(element).removeClass(errorClass);
			},

			// Different components require proper error label placement
            errorPlacement: function(error, element) {

                // Unstyled checkboxes, radios
                if (element.parents().hasClass('form-check')) {
                    error.appendTo( element.parents('.form-check').parent() );
                }

                // Input with icons and Select2
                else if (element.parents().hasClass('form-group-feedback') || element.hasClass('select2-hidden-accessible')) {
                    error.appendTo( element.parent() );
                }

                // Input group, styled file input
                else if (element.parent().is('.uniform-uploader, .uniform-select') || element.parents().hasClass('input-group')) {
                    error.appendTo( element.parent().parent() );
                }

                // Other elements
                else {
                    error.insertAfter(element);
                }
            },
			rules: {
				"bike[type]": {
					minlength: 1,
					maxlength: 128
				},
				"bike[brand]": {
					minlength: 1,
					maxlength: 128
				},
				"bike[model]": {
					minlength: 1,
					maxlength: 128
				},
				"bike[year]": {
					digits: true,
					minlength: 4,
					maxlength: 4,
					range: [1980, 2050]
				},
				"bike[value]": {
					minlength: 1,
					maxlength: 10
				},
				"bike[serial]": {
					minlength: 1,
					maxlength: 32
				},
				
				"answers[accessory]": {
					required :true
				},
				"answers[frequency]": {
					required :true
				},
				"answers[competition]": {
					required :true
				},
				"answers[where]": {
					required :true
				},
		
				"client[name]": {
					minlength: 1,
					maxlength: 128
				},
				"client[identity]": {
					minlength: 14,
					maxlength: 18
				},

				"client[contact][email]": {
					email:true,
					minlength: 1,
					maxlength: 64
				},
				"client[contact][phone1]": {
					minlength: 1,
					maxlength: 16
				},
				"client[contact][phone2]": {
					minlength: 1,
					maxlength: 16
				},
			},
			messages: {
				"bike[type]": {
					required: 'Tipo de bike obligatório'
				},
				"bike[brand]": {
					required: 'Marca de bike obligatório'
				},
				"bike[model]": {
					required: 'Modelo de bike obligatório'
				},
				"bike[year]": {
					required: 'Ano de fabricação obligatório',
					digits: "Ano inválido",
					range: jQuery.validator.format("Ano entre {0} e {1}"),
				},
				"bike[value]": {
					required: 'Valor estimado obligatório'
				},
				
				"answers[accessory]": {
					required: 'Resposta obligatório'
				},
				"answers[frequency]": {
					required: 'Resposta obligatório'
				},
				"answers[competition]": {
					required: 'Resposta obligatório'
				},
				"answers[where]": {
					required: 'Escolha obligatório'
				},
				
				"client[type]": {
					required: 'Tipo de cliente obligatório'
				 },
				
				"client[name]": {
					required: 'Nome do cliente obligatório'
				 },
				"client[identity]": {
					required: 'Numéro de CPF ou CNPJ obligatório',
					minlength: jQuery.validator.format("CPF ou CNPJ inválido"),
				},
				
				"client[contact][email]": {
					required: 'E-mail obligatório',
					email: "E-mail inválido"
				},
				"client[contact][phone1]": {
					required: 'Numéro de telefone obligatório'
				 },
				"client[contact][phone2]": {
					required: 'Numéro de telefone obligatório'
				 },
				 
				"client[address][zipcode]":  {
					required: 'CEP obligatório'
				},
				"client[address][streetname]":  {
					required: 'Endereço obligatório'
				},
				"client[address][streetnumber]":  {
					required: 'Numéro do endereço obligatório'
				},
				"client[address][neighborhood]":  {
					required: 'Bairro obligatório'
				},
				"client[address][city]":  {
					required: 'Cidade obligatório'
				},
				"client[address][state]":  {
					required: 'Estado obligatório'
				},
				"client[address][country]":  {
					required: 'País obligatório'
				}
		 }
	});
	
	//*** Client type change
	$('#client_identity').mask('000.000.000-00', {reverse: true});
	
	$("input:radio[name='client[type]']").change(function() {
		console.log('Change client type!');
		switch( $("input:radio[name='client[type]']:checked").val() ) {
			case 'PF' :
				$('#client_identity').val('');
				$("#lbl_client_name").html("Nome Completo: *");
				$("#lbl_client_identity").html("CPF: *");
				$("#client_name").attr("placeholder", "Digite o nome completo do cliente");
				$("#client_identity").attr("placeholder", "000.000.000-00");
				$('#client_identity').mask('000.000.000-00', {reverse: true});
				break;
			case 'PJ' :
				$('#client_identity').val('');
				$("#lbl_client_name").html("Razão Social: *");
				$("#lbl_client_identity").html("CNPJ: *");
				$("#client_name").attr("placeholder", "Digite a razão social do cliente");
				$("#client_identity").attr("placeholder", "00.000.000/0000-00");
				$('#client_identity').mask('00.000.000/0000-00', {reverse: true});
				break;
		}            
	});
	
	 $("#addForm").submit(function(e) {

		var form = $(this);
		var url = form.attr('action');

		$.ajax({
			   type: "POST",
			   url: url,
			   data: form.serialize(), // serializes the form's elements.
			   success: function(data)
			   {
				   console.log(data); 
				   $('#successNotif').fadeIn('slow').delay(5000).fadeOut('slow');
				   $('#addForm')[0].reset();
				   $(window).scrollTop($('#successNotif').offset().top -40);
			   }
			 });

		e.preventDefault(); // avoid to execute the actual submit of the form.
	});
	
		
		
	//*** Year Format
	$('#bike_year').mask("0000");
	
	//*** Money Format
	$('#bike_value').mask("#.##0,00", {reverse: true});
	
	
	//*** ZIP Code finder
	
	$('#client_zipcode').mask('00000-000');
	
	function limpa_formulário_cep() {
			//Limpa valores do formulário de cep.
			document.getElementById('client_streetname').value=("");
			document.getElementById('client_neighborhood').value=("");
			document.getElementById('client_city').value=("");
			document.getElementById('client_state').value=("");
			//document.getElementById('client_ibge').value=("");
	}

	function meu_callback(conteudo) {
		if (!("erro" in conteudo)) {
			//Atualiza os campos com os valores.
			document.getElementById('client_streetname').value=(conteudo.logradouro.toUpperCase());
			document.getElementById('client_neighborhood').value=(conteudo.bairro.toUpperCase());
			document.getElementById('client_city').value=(conteudo.localidade.toUpperCase());
			document.getElementById('client_state').value=(conteudo.uf.toUpperCase());
			//document.getElementById('client_ibge').value=(conteudo.ibge);
		} //end if.
		else {
			//CEP não Encontrado.
			limpa_formulário_cep();
			alert("CEP não encontrado.");
		}
	}
	
	function pesquisacep(valor) {

		//Nova variável "cep" somente com dígitos.
		var cep = valor.replace(/\D/g, '');

		//Verifica se campo cep possui valor informado.
		if (cep != "") {

			//Expressão regular para validar o CEP.
			var validacep = /^[0-9]{8}$/;

			//Valida o formato do CEP.
			if(validacep.test(cep)) {

				//Preenche os campos com "..." enquanto consulta webservice.
				document.getElementById('client_streetname').value="...";
				document.getElementById('client_neighborhood').value="...";
				document.getElementById('client_city').value="...";
				document.getElementById('client_state').value="...";
				//document.getElementById('client_ibge').value="...";

				//Cria um elemento javascript.
				var script = document.createElement('script');

				//Sincroniza com o callback.
				script.src = 'https://viacep.com.br/ws/'+ cep + '/json/?callback=meu_callback';


				//Insere script no documento e carrega o conteúdo.
				document.body.appendChild(script);
				
				$('#client_streetnumber').focus().select()

			} //end if.
			else {
				//cep é inválido.
				limpa_formulário_cep();
				alert("Formato de CEP inválido.");
			}
		} //end if.
		else {
			//cep sem valor, limpa formulário.
			limpa_formulário_cep();
		}
	};
	

