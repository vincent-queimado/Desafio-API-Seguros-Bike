  	<script>
	$(document).ready(function () {
		getInsurancesList();
	});
		
	//*** Check JS Plugins
	if (!$().steps) {
			console.warn('Warning - steps.min.js is not loaded.');
	}

	if (!$().validate) {
			console.warn('Warning - validate.min.js is not loaded.');
	}
	
	
	function getInsurancesList(){
			$.ajax({ 
				type: 'GET', 
				url: 'http://everguard.com.br:9090/seguros/api/insurances/list', 
				//data: { get_param: 'value' }, 
				dataType: 'json',
				success: function (data) { 
					var line = '';
					var i = 1;
					$('#insurances').empty();
					
					$.each(data, function(index, element) {
						
						var insurance_id = element._id;
						var insurance_date = moment(element.insurance.created_date).format("DD/MM/YYYY HH:MM");
						var insurance_name = element.insurance.name;
						var insurance_register_number = element.insurance.register_number;
						var insurance_expenses = element.insurance.expenses;
						var insurance_taxes = element.insurance.taxes;
						var insurance_profit = element.insurance.profit;
						var insurance_risk_factor = element.insurance.risk_factor;
						
						var insurance_risk_coverage = '';
						var geographical_coverage = '';
						
			
						var ar_risk_coverage = [];
						var ar_geographical_coverage = [];
						
						if (element.insurance.risk_coverage.loss == 'yes'){ar_risk_coverage.push('Perda')};
						if (element.insurance.risk_coverage.theft == 'yes'){ar_risk_coverage.push('Roubo e furto')};
						
						if (element.insurance.geographical_coverage.SP == 'yes'){ar_geographical_coverage.push('SP')};
						if (element.insurance.geographical_coverage.PE == 'yes'){ar_geographical_coverage.push('PE')};
						if (element.insurance.geographical_coverage.PB == 'yes'){ar_geographical_coverage.push('PB')};
						if (element.insurance.geographical_coverage.AL == 'yes'){ar_geographical_coverage.push('AL')};
						
						insurance_risk_coverage = ar_risk_coverage.join();
						insurance_geographical_coverage = ar_geographical_coverage.join();
						
						//console.log(insurance_risk_coverage);
						
						line =  "<div class='blog-comments__item d-flex p-3' style='padding-bottom: 8px!important; padding-top: 8px!important;'>"+
									  "<div style='width: 100%;' class='blog-comments__content'>"+
										"<div class='blog-comments__meta text-muted'>"+
										 "<div style='border-bottom: 1px solid #ccc'>"+
											"<div style='width: 90%; display: inline-block;'><span style='font-size: 0.7rem; color: #555 !important;' class=''>DETALHES DA SEGURADORA</span></div>"+
											"<div style='width: 10%; display: inline-block; text-align: right;'><span style='font-size: 0.7rem; color: #555 !important;' class=''><a style='color: #555 !important;' href='#' onclick='editInsurance(\"" + insurance_id + "\",\"" + insurance_name + "\" )'><i style='top: 1px;'class='material-icons'>create</i></a><a style='color: #555 !important;' href='#' onclick='delInsurance(\"" + insurance_id + "\",\"" + insurance_name + "\" )'><i style='top: 1px;'class='material-icons'>delete</i></a></span></div>"+
										  "</div>"+
										  "<div style=''><span style='font-size: 0.7rem;' class='text-muted'>DATA: " + insurance_date + "</span></div>"+
										  "<div style=''><span style='font-size: 0.7rem;' class='text-muted'>NOME: " + insurance_name + "</span></div>"+
										  "<div style=''><span style='font-size: 0.7rem;' class='text-muted'>CNPJ: " + insurance_register_number + "</span></div>"+
										  "<div style='border-bottom: 1px solid #ccc'>"+
											"<div style='width: 90%; display: inline-block;'><span style='font-size: 0.7rem; color: #555 !important;' class=''>DETALHES DE COBERTURA</span></div>"+
											"<div style='width: 10%; display: inline-block; text-align: right;'><span style='font-size: 0.7rem; color: #555 !important;' class=''></span></div>"+
										  "</div>"+
											"<div style=''><span style='font-size: 0.7rem;' class='text-muted'>Indenização: " + insurance_risk_coverage + "</span></div>"+
											"<div style=''><span style='font-size: 0.7rem;' class='text-muted'>cobertura territorial : " + insurance_geographical_coverage + "</span></div>"+
										  "<div style='border-bottom: 1px solid #ccc'>"+
											"<div style='width: 90%; display: inline-block;'><span style='font-size: 0.7rem; color: #555 !important;' class=''>COMPOSIÇÃO DO PRÊMIO</span></div>"+
											"<div style='width: 10%; display: inline-block; text-align: right;'><span style='font-size: 0.7rem; color: #555 !important;' class=''></span></div>"+
										  "</div>"+
											"<div style=''><span style='font-size: 0.7rem;' class='text-muted'>Probabilidade: " + insurance_risk_factor + "% (" + (1000*(insurance_risk_factor/100))+  "/1000 ano) </span></div>"+
											"<div style=''><span style='font-size: 0.7rem;' class='text-muted'>Despesas: " + insurance_expenses + "% </span></div>"+
											"<div style=''><span style='font-size: 0.7rem;' class='text-muted'>Impostos: " + insurance_taxes + "% </span></div>"+
											"<div style=''><span style='font-size: 0.7rem;' class='text-muted'>Margem: " + insurance_profit + "% </span></div>"+
										"</div>"+
										
										"<div style='text-align: center;width: 100%;margin-top: 10px;' class='blog-comments__actions'>"+
									
										 "</div>"+
									  "</div>"+
									"</div>";
									
						$('#insurances').append(line);
						
						i++;
					
					});
					
					if (i == 1){
						line = "<div class='blog-comments__item d-flex p-3'>"+
							"<div class='blog-comments__content'>"+
								"<div style='font-size: 0.8rem;' class='blog-comments__meta text-muted'>"+
									"Nenhuma seguradora encontrada!"+
								"</div>"+
							"</div>"+
						"</div>";
						$('#insurances').append(line);
					}
					
					
				}
			});
		}
		
	function getKeyByValue(object, value) {
	  return Object.keys(object).find(key => object[key] == value);
	}



	
	
	//*** Wizard Form
	var form_addclient = $("#addForm").show();
 
	form_addclient.steps({
		headerTag: "h3",
		bodyTag: "fieldset",
		transitionEffect: "slideLeft",
		titleTemplate: '<span class="number">#index#</span> #title#',
		labels: {
					previous: '<i class="icon-arrow-left13 mr-2" /> Anterior',
					next: 'Próximo <i class="icon-arrow-right14 ml-2" />',
					finish: 'Cadastrar <i class="icon-plus2 ml-2" />'
			},
			transitionEffect: 'fade',
			autoFocus: true,
			onStepChanging: function (event, currentIndex, newIndex) {

					// Allways allow previous action even if the current form is not valid!
					if (currentIndex > newIndex) {
							return true;
					}

					// Needed in some cases if the user went back (clean up)
					if (currentIndex < newIndex) {

							// To remove error styles
							form_addclient.find('.body:eq(' + newIndex + ') label.error').remove();
							form_addclient.find('.body:eq(' + newIndex + ') .error').removeClass('error');
					}

					form_addclient.validate().settings.ignore = ':disabled,:hidden';
					return form_addclient.valid();
			},
			onFinishing: function (event, currentIndex) {
					form_addclient.validate().settings.ignore = ':disabled';
					return form_addclient.valid();
			},
			onFinished: function (event, currentIndex) {
					//alert('Cadastrado!');
					form_addclient.submit();
			}
	});
	
		//*** Initialize validation
		form_addclient.validate({
				ignore: 'input[type=hidden], .select2-search__field', // ignore hidden fields
				errorClass: 'validation-invalid-label',
				highlight: function(element, errorClass) {
						$(element).removeClass(errorClass);
				},
				unhighlight: function(element, errorClass) {
						$(element).removeClass(errorClass);
				},

				// Different components require proper error label placement
				errorPlacement: function(error, element) {

					// Unstyled checkboxes, radios
					if (element.parents().hasClass('form-check')) {
						error.appendTo( element.parents('.form-check').parent() );
					}

					// Input with icons and Select2
					else if (element.parents().hasClass('form-group-feedback') || element.hasClass('select2-hidden-accessible')) {
						error.appendTo( element.parent() );
					}

					// Input group, styled file input
					else if (element.parent().is('.uniform-uploader, .uniform-select') || element.parents().hasClass('input-group')) {
						error.appendTo( element.parent().parent() );
					}

					// Other elements
					else {
						error.insertAfter(element);
					}
				},
				rules: {
					"insurance[name]": {
						required: true,
						minlength: 1,
						maxlength: 64
					},
					"insurance[register_number]": {
						required: true,
						minlength: 1,
						maxlength: 18
					},
					"insurance[expenses]": {
						required: true
					},
					"insurance[taxes]": {
						required: true
					},
					"insurance[profit]": {
						required: true
					},
					"insurance[risk_factor]": {
						required: true
					}
				},
			messages: {
				"insurance[name]": {
					required: 'Nome de Seguradora obligatório'
				 },
				"insurance[register_number]": {
					required: 'Numéro de CNPJ obligatório',
					minlength: jQuery.validator.format("CNPJ inválido")
				},
				"insurance[expenses]": {
					required: 'Despesas obligatório'
				},
				"insurance[taxes]": {
					required: 'Impostos obligatório'
				},
				"insurance[profit]": {
					required: 'Margem obligatório'
				},
				"insurance[risk_factor]": {
					required: true
				}
			}
		});
		
	$("#addForm").submit(function(e) {
		saveInsurance();
		e.preventDefault(); // avoid to execute the actual submit of the form.
	});
	
	function saveInsurance(){
		addInsurance(function(result){
			if (result){
				$('#successNotif').fadeIn('slow').delay(5000).fadeOut('slow');
				goBack();
				getInsurancesList();				
			}
		});
	}
	
			
	function addInsurance(result){
		$.ajax({
			type: "POST",
			url: "/seguros/api/insurances/add",
			data: $("#addForm").serialize(), // serializes the form's elements.
			success: function(data)
			{	
				result(data['_id']);
			}
		});
	}
	
	
	function goBack(){
		$('#addForm')[0].reset();
		$('#addForm-t-0').click();
		$(window).scrollTop(0);
	}
	
	
	function delInsurance(id, insurance_name){
			//alert('Deseja exluir definitivamente a oportunidade de ' + client + ' ?');
			
			bootbox.confirm({
                title: 'Confirmação',
                message: 'Deseja exluir definitivamente a seguradora  ' + insurance_name + ' ?',
                buttons: {
                    confirm: {
                        label: 'Sim',
                        className: 'btn-primary'
                    },
                    cancel: {
                        label: 'Não',
                        className: 'btn-link'
                    }
                },
                callback: function (result) {

					if (result){
						$.ajax({ 
							type: 'DELETE', 
							url: 'http://everguard.com.br:9090/seguros/api/insurances/delete/'+id, 
							//data: { get_param: 'value' }, 
							dataType: 'json',
							success: function (data) { 
								
								bootbox.alert({
									title: 'Confirmação',
									message: 'A seguradora ' + insurance_name + ' foi excluida com sucesso!'
								});
								
								getInsurancesList();
							}
						});
					}
					
	
                }
            });
			
		}
	
	
	
	//*** Uppercase Format
	$('#name').blur(function(){
		this.value = this.value.toUpperCase();
	});
	
	$('#name').keyup(function(){
		this.value = this.value.toUpperCase();
	});
	
	$('#register_number').mask('00.000.000/0000-00', {reverse: true});

	$('#expenses').mask("##0,00", {reverse: true});
	$('#taxes').mask("##0,00", {reverse: true});
	$('#profit').mask("##0,00", {reverse: true});
	
	</script>
		
