	<script>
	
		$(document).ready(function () {
			getQuoteCounts();
			getQuoteLatest();
			getInsuredLatest();
		});
		
		function getQuoteCounts(){
			$.ajax({ 
				type: 'GET', 
				url: 'http://everguard.com.br:9090/seguros/api/oportunities/count_pending', 
				dataType: 'json',
				success: function (data) { 
					$('#pending_quote').empty();
					$('#pending_quote').text(data[0]['count']);
				}
			});
			
			$.ajax({ 
				type: 'GET', 
				url: 'http://everguard.com.br:9090/seguros/api/oportunities/count_accepted',
				dataType: 'json',
				success: function (data) { 
					$('#accepted_quote').empty();
					$('#accepted_quote').text(data[0]['count']);
				}
			});
			
			$.ajax({ 
				type: 'GET', 
				url: 'http://everguard.com.br:9090/seguros/api/oportunities/count_refused', 
				dataType: 'json',
				success: function (data) { 
					$('#refused_quote').empty();
					$('#refused_quote').text(data[0]['count']);
				}
			});
		}
		
		
		
		function getQuoteLatest(){
			$.ajax({ 
				type: 'GET', 
				url: 'http://everguard.com.br:9090/seguros/api/oportunities/latest', 
				//data: { get_param: 'client' }, 
				dataType: 'json',
				success: function (data) { 
					var i = 1;
					
					$('#lastQuotes').empty();
					
					$.each(data, function(index, element) {
						if( element.oportunity.status == 'pending'){
							var oportunity_id = element._id;
							var oportunity_date = moment(element.oportunity.created_date).format("DD/MM/YYYY HH:mm");
							
							var client_id = element.oportunity.client;
							var client_name = '';
							var client_city = '';
							var client_state = '';
							
							var prime = parseFloat(element.oportunity.prime);
							
							getClientDatas(client_id,function(clientDatas){
								client_name = clientDatas.client.name;
								client_city = clientDatas.client.address.city;
								client_state = clientDatas.client.address.state;
								
								$('#lastQuotes').append("<li style='font-size: 0.7rem;' class='list-group-item d-flex px-3'>"+
														"<span style='white-space: nowrap;overflow: hidden;text-overflow: ellipsis;width: 80%;' class='text-semibold text-fiord-blue'>" +
														oportunity_date + " | " + client_name +
														"</span>" + 
														"<span class='ml-auto text-right text-semibold text-reagent-gray'>" +
														prime.toFixed(2).replace(".",",") +
														"</span></li>");
							});
		
							i++;
						}
					});
					
					if (i == 1){
						line = "<div class='blog-comments__item d-flex p-3'>"+
							"<div class='blog-comments__content'>"+
								"<div style='font-size: 0.8rem;' class='blog-comments__meta text-muted'>"+
									"Nenhuma oportunidade encontrada!"+
								"</div>"+
							"</div>"+
						"</div>";
						$('#lastQuotes').append(line);
					}
					
				}
			});
		}
		
		
		
		function getInsuredLatest(){
			$.ajax({ 
				type: 'GET', 
				url: 'http://everguard.com.br:9090/seguros/api/oportunities/list_accepted', 
				//data: { get_param: 'client' }, 
				dataType: 'json',
				success: function (data) { 
					var i = 1;
					
					$('#lastInsured').empty();
					
					$.each(data, function(index, element) {
					
						var oportunity_id = element._id;
						var oportunity_date = moment(element.oportunity.created_date).format("DD/MM/YYYY HH:mm");
						
						var client_id = element.oportunity.client;
						var client_name = '';
						var client_city = '';
						var client_state = '';
						
						var prime = parseFloat(element.oportunity.prime);
						
						getClientDatas(client_id,function(clientDatas){
							client_name = clientDatas.client.name;
							client_city = clientDatas.client.address.city;
							client_state = clientDatas.client.address.state;
							
							$('#lastInsured').append("<li style='font-size: 0.7rem;' class='list-group-item d-flex px-3'>"+
													"<span style='white-space: nowrap;overflow: hidden;text-overflow: ellipsis;width: 80%;' class='text-semibold text-fiord-blue'>" +
													oportunity_date + " | " + client_name +
													"</span>" + 
													"<span class='ml-auto text-right text-semibold text-reagent-gray'>" +
													prime.toFixed(2).replace(".",",") +
													"</span></li>");
						});
	
						i++;
					});
					
					if (i == 1){
						line = "<div class='blog-comments__item d-flex p-3'>"+
							"<div class='blog-comments__content'>"+
								"<div style='font-size: 0.8rem;' class='blog-comments__meta text-muted'>"+
									"Nenhum segurado encontrado!"+
								"</div>"+
							"</div>"+
						"</div>";
						$('#lastInsured').append(line);
					}
				}
			});
		}
		
		
		
		
		function getClientDatas(id, datas){
			$.ajax({ 
				type: 'GET', 
				url: 'http://everguard.com.br:9090/seguros/api/clients/view/'+id, 
				//data: { get_param: 'value' }, 
				dataType: 'json',
				success: function (data) { 
					datas(data);
				}
			});
		}
		
		
